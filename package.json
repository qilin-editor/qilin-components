{
  "name": "qilin-components",
  "version": "0.0.1",
  "description":
    "A bunch of common components used by Qilin editor and Qilin plugins",
  "keywords": ["qilin", "qilin-theme", "qilin-plugin", "qilin-components"],
  "author":
    "Łaniewski Bartosz <laniewski.bartozzz@gmail.com> (http://laniewski.me/)",
  "license": "MIT",
  "engines": {
    "node": ">=8.0",
    "npm": ">=5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qilin-editor/qilin-components"
  },
  "bugs": {
    "url": "https://github.com/qilin-editor/qilin-components/issues"
  },
  "scripts": {
    "prepare": "npm run lint:scripts && npm run lint:styles && npm run start",
    "start": "npm run build",
    "build": "webpack --progress --hide-modules --config config.build.js",
    "watch": "webpack-dev-server --progress --config config.watch.js",
    "lint:scripts": "eslint --fix ./src",
    "lint:styles": "stylelint './src/**/*.js'",
    "flow": "flow"
  },
  "dependencies": {
    "grid-styled": "^3.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^4.0.0",
    "styled-components": "^3.2.1",
    "styled-is": "^1.1.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.67.1",
    "html-webpack-plugin": "^3.0.6",
    "prettier": "^1.11.1",
    "stylelint": "^9.1.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.0",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "maintainers": [
    {
      "name": "Bartosz Łaniewski",
